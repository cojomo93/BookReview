<GridPane alignment="center" hgap="10" prefHeight="241.0" prefWidth="303.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookreviewapp.NavigationUICntl">
    <padding>
        <Insets bottom="10" left="25" right="25" top="25" />
    </padding>
    <children>   
        <Button fx:id="setReminder" onAction="#handleSetReminderButtonAction" text="See Book Reviews" GridPane.rowIndex="1" />      
        <Button onAction="#handleExitButtonAction" prefHeight="25.0" prefWidth="76.0" text="Exit" GridPane.rowIndex="4" />  
        <Text fx:id="actiontarget" GridPane.columnIndex="1" GridPane.rowIndex="6" />
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="Book Reviews" textAlignment="CENTER">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
            </children>
        </VBox>
    </children>
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
